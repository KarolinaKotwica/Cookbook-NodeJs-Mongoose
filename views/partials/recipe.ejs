<div class="recipes">
    <% recipes.forEach(recipe => { %>
    <div class="card">
        <a href="/przepisy/<%= recipe._id %>">
            <img src="https://img-store223.s3.amazonaws.com/<%= recipe.image %>" class="card-img-top" alt="<%= recipe.title %>">
            <div class="card-body">
                    <h5><%= recipe.title %></h5>
                    <p class="card-text">
                        <!-- <span><%= recipe.category %></span> -->
                        <span><%= recipe.time %> min</span>
                        <span><%= recipe.created.getDate() %> / <%= recipe.created.getMonth()+1 %> / <%= recipe.created.getFullYear() %></span>
                    </p>
            </div>
        </a>
    </div>
    <% }) %> 
</div>